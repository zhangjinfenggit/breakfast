package com.hmw.open.web.weixin.request.ordercontroller;

import java.math.BigDecimal;
import java.util.List;

public class SaveOrderRequest {
	private List<PackProduct> packProductList;
	private List<SingleProduct> singleProductList;
	private Long shopId;
//	private Long couponId;
//	private BigDecimal couponMoney;
	private BigDecimal totalMoney;
	
	

	
	
	public List<PackProduct> getPackProductList() {
		return packProductList;
	}
	public void setPackProductList(List<PackProduct> packProductList) {
		this.packProductList = packProductList;
	}
	public List<SingleProduct> getSingleProductList() {
		return singleProductList;
	}
	public void setSingleProductList(List<SingleProduct> singleProductList) {
		this.singleProductList = singleProductList;
	}
	public Long getShopId() {
		return shopId;
	}
	public void setShopId(Long shopId) {
		this.shopId = shopId;
	}
	public BigDecimal getTotalMoney() {
		return totalMoney;
	}
	public void setTotalMoney(BigDecimal totalMoney) {
		this.totalMoney = totalMoney;
	}


}
