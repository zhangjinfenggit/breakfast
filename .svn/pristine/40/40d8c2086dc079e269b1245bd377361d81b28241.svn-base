<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hmw.open.mapper.BfSubwayMapper">

	<resultMap id="BaseResultMap" type="com.hmw.open.model.BfSubway">
		<id column="id" property="id" />
		<result column="city_id" property="cityId" />
		<result column="subway_name" property="subwayName" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, city_id AS cityId, subway_name AS subwayName
    </sql>
</mapper>
