package com.hmw.open.web.app.request.merchantsinfocontroller;

/***
 * 售罄或者打烊请求参数
 * @author kongliufeng
 *
 */
public class OpenOrCloseGoodRequest {
	private Integer id;//套餐或单品id
	private int type;//类型标识，1.表示商品，2.表示套餐
	private int operationCode;//操作标识码，1.表示操作售罄, 2.表示操作补货
	
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public int getType() {
		return type;
	}
	public void setType(int type) {
		this.type = type;
	}
	public int getOperationCode() {
		return operationCode;
	}
	public void setOperationCode(int operationCode) {
		this.operationCode = operationCode;
	}
	}
