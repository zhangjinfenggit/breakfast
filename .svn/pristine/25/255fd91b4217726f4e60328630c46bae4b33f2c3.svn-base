package com.hmw.open.web.app.request.merchantsinfocontroller;


/***
 * 订单处理的请求参数
 * @author kongliufeng
 *
 */
public class OrderDealRequest {
	
	private String opType;//1 备餐完成、2 取消订单、3领取、4同意退款、5 拒绝
	private String orderNo;//订单号
	private String cancleMessage;//取消订单说明
	private String refundDetail;//退款说明

	public String getOpType() {
		return opType;
	}
	public void setOpType(String opType) {
		this.opType = opType;
	}
	public String getOrderNo() {
		return orderNo;
	}
	public void setOrderNo(String orderNo) {
		this.orderNo = orderNo;
	}
	public String getCancleMessage() {
		return cancleMessage;
	}
	public void setCancleMessage(String cancleMessage) {
		this.cancleMessage = cancleMessage;
	}
	public String getRefundDetail() {
		return refundDetail;
	}
	public void setRefundDetail(String refundDetail) {
		this.refundDetail = refundDetail;
	}
	
}
