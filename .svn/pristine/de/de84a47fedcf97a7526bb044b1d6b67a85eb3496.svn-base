$(function(){
	var phone = sessionStorage.getItem('phone');
	
	//订单金额
	var ordersMoney = GetQueryString("ordersMoney");
	if(ordersMoney == null || ordersMoney == ""){
		alert("请传入[ordersMoney]参数")
	}
	
	var _data = {
		"ordersMoney":ordersMoney,
		"phone":phone
	}
	$.ajax({
		type:"post",
		url:"http://"+ip+"/breakfastApi/weixin/queryOrderHistory",
		async:true,
		data:JSON.stringify(_data),
		dataType:"json",
		success:function(result){
			if(result.retCode == "000000"){
				var list = result.data;
				var str = "";
				for(var i=0;i<list.length;i++){
					str += "<li><img src='images/coupon.png' alt='' class='coupon'>"+
							"<div class='cou_desc'><p class='cou_price'>¥"+list[i].couponMoney+"</p>"+
							"<p>早餐优惠券</p><span>限"+list[i].overdueTime+"日前使用</span>"+
							"<span>使用此劵可抵扣"+list[i].couponMoney+"元早餐费用</span></div>"+
							"<img src='images/current.png' alt='' class='current'></li>";
				}
				$("#couponList").html(str);
			}
		}
	});
});