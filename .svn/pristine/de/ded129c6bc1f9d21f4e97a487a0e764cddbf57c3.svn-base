<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hmw.open.mapper.BfSubwayLineMapper">

	<resultMap id="BaseResultMap" type="com.hmw.open.model.BfSubwayLine">
		<id column="id" property="id" />
		<result column="city_name" property="cityName" />
		<result column="subway_line" property="subwayLine" />
		<result column="subway_name" property="subwayName" />
		<result column="subway_out" property="subwayOut" />
		<result column="operater" property="operater" />
		<result column="create_time" property="createTime" />
		<result column="update_time" property="updateTime" />
		<result column="remark" property="remark" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, city_name AS cityName, subway_line AS subwayLine, subway_name AS subwayName, subway_out AS subwayOut, operater, create_time AS createTime, update_time AS updateTime, remark
    </sql>
</mapper>
