<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bf.open.mapper.BfSingeProductMapper">

	<resultMap id="BaseResultMap" type="com.hmw.open.model.BfSingeProduct">
		<id column="id" property="id" />
		<result column="single_name" property="singleName" />
		<result column="single_money" property="singleMoney" />
		<result column="single_amount" property="singleAmount" />
		<result column="real_amount" property="realAmount" />
		<result column="status" property="status" />
		<result column="photo" property="photo" />
		<result column="shop_id" property="shopId" />
		<result column="shop_name" property="shopName" />
		<result column="single_type" property="singleType" />
		<result column="operater" property="operater" />
		<result column="create_time" property="createTime" />
		<result column="update_time" property="updateTime" />
		<result column="remark" property="remark" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, single_name AS singleName, single_money AS singleMoney, single_amount AS singleAmount, real_amount AS realAmount, status, photo, shop_id AS shopId, shop_name AS shopName, single_type AS singleType, operater, create_time AS createTime, update_time AS updateTime, remark
    </sql>
</mapper>
