package com.hmw.open.model;

import java.math.BigDecimal;
import java.util.List;

public class BfBusinessRecordDayVo {

	/**
	 * 历史总收入
	 */
	private BigDecimal historySumIncome;

	/**
	 * 历史总退款
	 */
	private BigDecimal historySumRefund;

	/**
	 * 历史总分成
	 */
	private BigDecimal historySumPartion;

	/**
	 * 合计
	 */
	private BigDecimal total;

	/**
	 * 详细信息
	 */
	private List<HistroySumRecordVo> histroySumRecordVos;

	public BigDecimal getHistorySumIncome() {
		return historySumIncome;
	}

	public void setHistorySumIncome(BigDecimal historySumIncome) {
		this.historySumIncome = historySumIncome;
	}

	public BigDecimal getHistorySumRefund() {
		return historySumRefund;
	}

	public void setHistorySumRefund(BigDecimal historySumRefund) {
		this.historySumRefund = historySumRefund;
	}

	public BigDecimal getHistorySumPartion() {
		return historySumPartion;
	}

	public void setHistorySumPartion(BigDecimal historySumPartion) {
		this.historySumPartion = historySumPartion;
	}

	public BigDecimal getTotal() {
		return total;
	}

	public void setTotal(BigDecimal total) {
		this.total = total;
	}

	public List<HistroySumRecordVo> getHistroySumRecordVos() {
		return histroySumRecordVos;
	}

	public void setHistroySumRecordVos(List<HistroySumRecordVo> histroySumRecordVos) {
		this.histroySumRecordVos = histroySumRecordVos;
	}

}
