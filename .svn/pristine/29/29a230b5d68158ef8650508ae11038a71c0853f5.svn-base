package com.hmw.open.common.enums;
/**
 * 
 * 类型（平台:0、监管:1、学校:2、运营:3、学校且运营:4、维修公司:5、保险公司:6）
 * @author bing.cheng
 *
 */
public enum CompanyTypeEnums {

	Platform(0, "平台"),
	Supervision(1, "监管"), //
	School(2, "学校"), 
	operation(3, "运营"), 
	SchoolAndOperation(4, "学校且运营"), 
	MaintenanceCompany(5, "维修公司"), 
	InsuranceCompany(6, "保险公司");
	
	private Integer code;
	private String desc;
	
	CompanyTypeEnums(Integer code, String desc) {
		this.code = code;
		this.desc = desc;
	}

	public Integer getCode() {
		return code;
	}

	public void setCode(Integer code) {
		this.code = code;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}
	

}
