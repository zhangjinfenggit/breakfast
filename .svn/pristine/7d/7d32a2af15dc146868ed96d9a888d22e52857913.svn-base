<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hmw.open.mapper.BfPackageProductMapper">

	<resultMap id="BaseResultMap" type="com.hmw.open.model.BfPackageProduct">
		<id column="id" property="id" />
		<result column="package_name" property="packageName" />
		<result column="package_money" property="packageMoney" />
		<result column="package_amount" property="packageAmount" />
		<result column="real_amount" property="realAmount" />
		<result column="pack_status" property="packStatus" />
		<result column="shop_id" property="shopId" />
		<result column="name" property="name" />
		<result column="single_id" property="singleId" />
		<result column="operater" property="operater" />
		<result column="create_time" property="createTime" />
		<result column="update_time" property="updateTime" />
		<result column="remark" property="remark" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, package_name AS packageName, package_money AS packageMoney, package_amount AS packageAmount, real_amount AS realAmount, pack_status AS packStatus, shop_id AS shopId, name, single_id AS singleId, operater, create_time AS createTime, update_time AS updateTime, remark
    </sql>
</mapper>
