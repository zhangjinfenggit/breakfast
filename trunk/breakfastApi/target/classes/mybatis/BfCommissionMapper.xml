<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hmw.open.mapper.BfCommissionMapper">

	<resultMap id="BaseResultMap" type="com.hmw.open.model.BfCommission">
		<id column="id" property="id" />
		<result column="extract_money" property="extractMoney" />
		<result column="section_left" property="sectionLeft" />
		<result column="section_right" property="sectionRight" />
		<result column="order_num" property="orderNum" />
		<result column="operater" property="operater" />
		<result column="create_time" property="createTime" />
		<result column="update_time" property="updateTime" />
		<result column="remark" property="remark" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, extract_money AS extractMoney, section_left AS sectionLeft, section_right AS sectionRight, order_num AS orderNum, operater, create_time AS createTime, update_time AS updateTime, remark
    </sql>
</mapper>
