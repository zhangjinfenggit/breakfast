<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hmw.open.mapper.BfCommissionRuleMapper">

	<resultMap id="BaseResultMap" type="com.hmw.open.model.BfCommissionRule">
		<id column="id" property="id" />
		<result column="rule_key" property="ruleKey" />
		<result column="rule_value" property="ruleValue" />
		<result column="operater" property="operater" />
		<result column="create_time" property="createTime" />
		<result column="update_time" property="updateTime" />
		<result column="remark" property="remark" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, rule_key AS ruleKey, rule_value AS ruleValue, operater, create_time AS createTime, update_time AS updateTime, remark
    </sql>
</mapper>
